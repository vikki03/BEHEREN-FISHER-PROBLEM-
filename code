t6=c(62,65,66,64,64,66,65,62,63,64,65,68,60)
N1=13
N2=15
v=sqrt(N1/N2)
y1=(t1-v*t4)
y2=(t2-v*t5)
y3=(t3-v*t6)
y1=c(13.4550,-0.0613,5.5932,3.8005,2.5241,1.6623,7.8696,6.7314,5.4550,9.5932,10.5241,10.8005,1.0078)
y2=c(13.9061,4.3207,7.9061,2.1825,5.0443,4.1134,3.3207,6.1134,6.9752,11.0443,9.1134,12.9752,10.8370)
y3=c(12.2842,3.4915,4.3533,10.4224,5.4224,2.5606,11.4915,12.2842,5.3533,5.4224,8.4915,4.6988,10.1460)
